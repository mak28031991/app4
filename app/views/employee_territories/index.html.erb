<p id="notice"><%= notice %></p>

<h1>Listing Employee Territories</h1>

<table>
  <thead>
    <tr>
      <th>Employeeid</th>
      <th>Territoryid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employee_territories.each do |employee_territory| %>
      <tr>
        <td><%= employee_territory.EmployeeID %></td>
        <td><%= employee_territory.TerritoryID %></td>
        <td><%= link_to 'Show', employee_territory %></td>
        <td><%= link_to 'Edit', edit_employee_territory_path(employee_territory) %></td>
        <td><%= link_to 'Destroy', employee_territory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employee territory', new_employee_territory_path %>
